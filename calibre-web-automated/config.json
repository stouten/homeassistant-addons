{
  "name": "Calibre Web Automated",
  "version": "V3.0.4",
  "slug": "calibre-web-automated",
  "description": "Home Assistant add-on voor Calibre Web Automated, gebaseerd op de officiÃ«le multi-arch Docker image.",
  "arch": [
    "amd64",
    "armhf",
    "armv7",
    "aarch64"
  ],
  "init": false,
  "ingress": true,
  "ingress_port": 8083,
  "ports": {
    "8083/tcp": 8083
  },
  "ports_description": {
    "8083/tcp": "Calibre Web Automated web interface"
  },
  "startup": "application",
  "environment": {
    "PUID": "1000",
    "PGID": "1000",
    "TZ": "Europe/Amsterdam"
  },
  "map": [
    "config:rw",
    "ssl:rw",
    "media:rw"
  ],
  "options": {
    "calibre_library_path": "/calibre-library",
    "book_ingest_path": "/cwa-book-ingest"
  },
  "schema": {
    "calibre_library_path": "str",
    "book_ingest_path": "str"
  },
  "image": "docker.io/crocodilestick/calibre-web-automated"
}
